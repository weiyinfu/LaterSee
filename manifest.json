{
  "name": "OneTab",
  "description": "自定义onetab",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "128": "onetab.png",
    "32": "onetab.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "lib.js",
      "axios.min.js",
      "haha.js"
    ]
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "activeTab",
    "http://*/",
    "http://*/"
  ]
}
